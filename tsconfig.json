{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es2020",
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
"resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "./dist"
    },
    "lib": ["es2015"]
    ,
    "include": ["src/**/*.ts", "./server.ts"]
    ,
    "exclude": ["node_modules", "dist"]
  }